#include<iostream>
#include<iomanip>

using namespace std;

float f(float x)
{
return 1/(1+x*x);
}

int main()
{
float x0,xn,h,s,k;
int i,n;

cout<<"Enter the x0,xn, no. of subintervals"<<endl;
cin>>x0>>xn>>n;
// cout<<fixed;

h=(xn-x0)/n;
s=f(x0)+f(xn);

for(i=1;i<=n-1; i++){
k=x0+i*h;

    s=s+2*f(k);

}

cout<<"Value of integral is ";

cout<<(h/2)*s<<endl;
return 0;
}
