#include<iostream>
#include<iomanip>

using namespace std;

float f(float x)
{
return 1/(1+x*x);
}

int main()
{
float x0,xn,h,s;
int i,n;

cout<<"Enter the x0,xn, no. of subintervals"<<endl;
cin>>x0>>xn>>n;
cout<<fixed;

h=(xn-x0)/n;
s=f(x0)+f(xn)+4*f(x0+h);

for(i=3;i<=n-1;i=i+2)
s +=4*f(x0+i*h)+2*f(x0+(i-1)*h);


cout<<"Value of integral is ";

cout<<(h/3)*s<<endl;
return 0;
}
